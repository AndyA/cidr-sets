{
  "name": "cidr-sets",
  "version": "0.0.1",
  "description": "Boolean operations on sets of CIDR ranges",
  "type": "module",
  "scripts": {
    "test": "jest",
    "tdd": "jest --watchAll",
    "build": "tsc",
    "clean": "rm -rf ./built",
    "prettier": "prettier -w **/*.js **/*.ts",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AndyA/cidr-sets.git"
  },
  "author": "Andy Armstrong <andy@hexten.net>",
  "bugs": {
    "url": "https://github.com/AndyA/cidr-sets/issues"
  },
  "homepage": "https://github.com/AndyA/cidr-sets#readme",
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": false,
    "quoteProps": "consistent",
    "jsxSingleQuote": false,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "requirePragma": false,
    "insertPragma": false,
    "proseWrap": "preserve",
    "htmlWhitespaceSensitivity": "css",
    "endOfLine": "lf"
  },
  "private": true,
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.192",
    "@types/node": "^18.15.11",
    "@types/printf": "^0.3.1",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint": "^8.38.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "printf": "^0.6.1"
  }
}
